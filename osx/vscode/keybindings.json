// Place your key bindings in this file to overwrite the defaults
[
  {
    "key": "ctrl+shift+tab",
    "command": "workbench.action.previousEditor",
    "when": ""
  },
  {
    "key": "ctrl+tab",
    "command": "workbench.action.nextEditor",
    "when": ""
  },
  {
    "key": "cmd+y",
    "command": "redo",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+cmd+z",
    "command": "-redo",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+shift+,",
    "command": "moveActiveEditor",
    "args": {
      "to": "left",
      "by": "tab",
      "value": 1
    }
  },
  {
    "key": "ctrl+shift+.",
    "command": "moveActiveEditor",
    "args": {
      "to": "right",
      "by": "tab",
      "value": 1
    }
  },
  {
    "key": "ctrl+shift+up",
    "command": "cursorMove",
    "args": {
      "to": "up",
      "by": "line",
      "value": 10
    },
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+down",
    "command": "cursorMove",
    "args": {
      "to": "down",
      "by": "line",
      "value": 10
    },
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+i",
    "command": "workbench.panel.chatSidebar"
  },
  {
    "key": "shift+cmd+l",
    "command": "-aichat.insertselectionintochat"
  },
  {
    "key": "shift+cmd+e",
    "command": "-aichat.fixerrormessage",
    "when": "(arbitrary function)"
  },
  {
    "key": "ctrl+shift+cmd+g",
    "command": "workbench.view.extension.github-pull-requests"
  },
  {
    "key": "cmd+i",
    "command": "composerMode.agent"
  }
]
